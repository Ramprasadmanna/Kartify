{
  "name": "rststore",
  "version": "1.0.0",
  "description": "Rst Store Application",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "node server/main.js",
    "server": "nodemon server/main.js",
    "client": "npm run dev --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node server/seeder",
    "data:destroy": "node server/seeder -d"
  },
  "imports": {
    "#config/*": "./server/config/*",
    "#data/*": "./server/data/*",
    "#models/*": "./server/models/*",
    "#routes/*": "./server/routes/*",
    "#middlewares/*": "./server/middlewares/*",
    "#controllers/*": "./server/controllers/*",
    "#utils/*": "./server/utils/*"
  },
  "author": "Ramprasad Manna",
  "license": "ISC",
  "dependencies": {
    "@babel/preset-env": "^7.26.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^5.0.0-beta.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "otp-generator": "^4.0.1",
    "razorpay": "^2.9.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3"
  }
}
